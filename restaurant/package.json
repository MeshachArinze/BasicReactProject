{
  "name": "restaurant",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "run-p dev start-api",
    "start:real-api": "run-p dev start-api",
    "dev:msw": "cross-env VITE_ENABLE_DEVTOOLS=Y vite",
    "start-api": "json-server --watch db.json --port 3001 --delay 1000",
    "test": "vitest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cy": "start-test 5173 cy:open",
    "cy:open": "cypress open",
    "tailwind": "npx tailwindcss -i ./src/index.css -o ./src/App.css --watch"
  },
  "dependencies": {
    "@storybook/addon-actions": "^7.0.20",
    "@storybook/addon-essentials": "^7.0.20",
    "@storybook/addon-interactions": "^7.0.20",
    "@storybook/addon-links": "^7.0.20",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-vite": "^7.0.20",
    "@storybook/react": "^7.0.20",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/testing-react": "^2.0.1",
    "@tanstack/react-query": "^4.29.12",
    "@tanstack/react-query-devtools": "^4.29.12",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "cypress": "^12.14.0",
    "jsdom": "^22.1.0",
    "json-server": "^0.17.3",
    "msw": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.10",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.12.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
